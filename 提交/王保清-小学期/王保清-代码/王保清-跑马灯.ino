void setup() {
pinMode(8,OUTPUT);
pinMode(7,OUTPUT);
pinMode(4,OUTPUT);
}
void loop() {
digitalWrite(8,HIGH);
delay(300);
digitalWrite(8,HIGH);
delay(10);
digitalWrite(7,HIGH);
delay(300);
digitalWrite(7,HIGH);
delay(10);
digitalWrite(4,HIGH);
delay(300);
digitalWrite(4,HIGH);
delay(10);
}
